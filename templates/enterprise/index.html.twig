{% extends 'base.html.twig' %}

{% block title %}{{title_page}}{% endblock %}

{% block body %}
<h1>{{title_page}}</h1>

{% if enterprises| default %}
<div class="ui list">
    {% for enterprise in enterprises %}
    <div class="item">
        <i class="folder icon"></i>
        <div class="content">
            <div class="header">{{ enterprise }}</div>
        </div>
    </div>
    {% endfor %}
</div>
{% endif %}

{% endblock %}