{% extends 'base.html.twig' %}

{% block title %}Password Reset Email Sent{% endblock %}

{% block body %}
<p>
    {% for delai in resetToken.expirationMessageData %}
    {{ 'reset_password.expiration_message_key'|trans({'%expiration_interval%': delai}, 'ResetPasswordBundle') }}
    {% endfor %}
</p>
<p>If you don't receive an email please check your spam folder or <a
        href="{{ path('app_forgot_password_request') }}">try again</a>.</p>
{% endblock %}